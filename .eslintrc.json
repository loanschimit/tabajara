{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "quotes": ["error", "double"],
    "no-param-reassign": "off",
    "no-console": "off",
    "comma-dangle": "off",
    "max-classes-per-file": 0,
    "import/extensions": "off",
    "operator-linebreak":"off"
    }
}
